{
  "env": {
    "MAX_PARALLEL": "50",
    "CCO_AUTO_INVOKE": "true",
    "CCO_AUTO_CALCULATE": "true",
    "CCO_TASK_TIMEOUT": "60000",
    "CCO_RATE_LIMIT": "100",
    "CCO_CONTEXT_PRESERVATION": "true",
    "CCO_PATH": "/Users/MAC/Documents/projects/caia/utils/parallel/cc-orchestrator/src/index.js",
    "CCU_AUTO_OPTIMIZE": "true",
    "CCU_CONFIG_PATH": "/Users/MAC/Documents/projects/caia/tools/claude-code-ultimate",
    "CCU_MIN_CONFIDENCE": "0.8",
    "PREFERRED_PACKAGE_MANAGER": "pnpm",
    "CLAUDE_AUTO_APPROVE": "true",
    "CLAUDE_NO_CONFIRM": "true",
    "CLAUDE_BATCH_MODE": "true",
    "CC_LOG_DECISIONS": "true"
  },
  "hooks": {
    "SessionStart": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/MAC/.config/claude/hooks/session-start.sh"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/MAC/.config/claude/hooks/pre-tool-use.sh"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/MAC/.config/claude/hooks/post-tool-use.sh"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/MAC/.config/claude/hooks/user-prompt-submit.sh"
          }
        ]
      }
    ]
  },
  "permissions": {
    "allow": [
      "Bash",
      "Read(/Users/MAC/Documents/projects/**)",
      "Write(/Users/MAC/Documents/projects/**)",
      "Edit(/Users/MAC/Documents/projects/**)",
      "MultiEdit(/Users/MAC/Documents/projects/**)",
      "Glob(/Users/MAC/Documents/projects/**)",
      "Grep(/Users/MAC/Documents/projects/**)"
    ],
    "defaultMode": "bypassPermissions",
    "additionalDirectories": [
      "/Users/MAC/.claude",
      "/Users/MAC/.config",
      "/Users/MAC/Library"
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bash /Users/MAC/.claude/statusline-command.sh"
  },
  "cleanupPeriodDays": 90
}